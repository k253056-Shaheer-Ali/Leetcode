#include <stdio.h>

void moveZeroes(int nums[], int size) {
    int i = 0; 

    // Step 1: Move all non-zero elements to the front
    for (int j = 0; j < size; j++) {
        if (nums[j] != 0) {
            nums[i] = nums[j];
            i++;
        }
    }

    // Step 2: Fill the rest with zeroes
    while (i < size) {
        nums[i] = 0;
        i++;
    }
}

int main() {
    int nums[] = {0, 1, 0, 3, 12};
    int size = sizeof(nums) / sizeof(nums[0]);

    moveZeroes(nums, size);

    // Print result
    printf("After moving zeroes: ");
    for (int i = 0; i < size; i++) {
        printf("%d ", nums[i]);
    }
    printf("\n");

    return 0;
}
